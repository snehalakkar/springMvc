<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>loginRegistraionProjectnew</display-name>
  <welcome-file-list>
    <welcome-file>wel</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>welcome</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.WelcomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>welcome</servlet-name>
		<url-pattern>/wel</url-pattern>
	</servlet-mapping>
  <servlet>
		<servlet-name>third</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.RegistrationPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>third</servlet-name>
		<url-pattern>/reg</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>forth</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.GetEmployeeDetails</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>forth</servlet-name>
		<url-pattern>/details</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>fifth</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.UserRegistration</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fifth</servlet-name>
		<url-pattern>/user_reg</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>sixth</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.UserLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sixth</servlet-name>
		<url-pattern>/user_login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>profile</servlet-name>
		<servlet-class>com.bridgeLabz.loginRegistraionProject.Profile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>Filter</filter-name>
		<filter-class>com.bridgeLabz.loginRegistraionProject.MyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Filter</filter-name>
		<url-pattern>/profile</url-pattern>
		<url-pattern>/details</url-pattern>
		<url-pattern>/reg</url-pattern>
	</filter-mapping>
</web-app>